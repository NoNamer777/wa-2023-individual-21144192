{
    "name": "vue-project",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "vite ../../dist/vue-project --mode=production --host --port=4200 --strictPort",
        "build": "run-p type-check build-only",
        "build-dev": "vite build --watch",
        "test": "vitest",
        "test-dev": "vitest --watch",
        "deploy": "cross-env DEPLOYING=true npm run build && npm run prepare-404",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "prepare-404": "cd ../../dist/vue-project && cp index.html 404.html",
        "lint": "eslint \"./src/**/*{.js,ts,vue,html}\""
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.3.0",
        "@fortawesome/free-solid-svg-icons": "^6.3.0",
        "@fortawesome/vue-fontawesome": "^3.0.3",
        "@popperjs/core": "^2.11.6",
        "bootstrap": "^5.2.3",
        "pinia": "^2.0.28",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@pinia/testing": "^0.0.15",
        "@rushstack/eslint-patch": "^1.1.4",
        "@types/jsdom": "^20.0.1",
        "@types/node": "^18.11.12",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitest/coverage-istanbul": "^0.28.5",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/test-utils": "^2.3.0",
        "@vue/tsconfig": "^0.1.3",
        "cross-env": "^7.0.3",
        "eslint-plugin-vue": "^9.3.0",
        "jsdom": "^20.0.3",
        "npm-run-all": "^4.1.5",
        "sass": "^1.58.3",
        "typescript": "~4.7.4",
        "vite": "^4.0.0",
        "vite-plugin-checker": "^0.5.6",
        "vitest": "^0.25.6",
        "vue-tsc": "^1.0.12"
    }
}
