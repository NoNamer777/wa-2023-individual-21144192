{
    "name": "vue-project",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "vue-project:start": "servor ../../dist/vue-project index.html 4200 --reload",
        "vue-project:build-dev": "vite build --watch",
        "vue-project:build": "run-p vue-project:type-check vue-project:build-only",
        "vue-project:build-only": "vite build",
        "vue-project:type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "vue-project:test-dev": "vitest --environment jsdom --root src/"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.3.0",
        "@fortawesome/free-solid-svg-icons": "^6.3.0",
        "@fortawesome/vue-fontawesome": "^3.0.3",
        "@popperjs/core": "^2.11.6",
        "bootstrap": "^5.2.3",
        "pinia": "^2.0.28",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.1.4",
        "@types/jsdom": "^20.0.1",
        "@types/node": "^18.11.12",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitejs/plugin-vue-jsx": "^3.0.0",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/test-utils": "^2.2.6",
        "@vue/tsconfig": "^0.1.3",
        "eslint-plugin-vue": "^9.3.0",
        "jsdom": "^20.0.3",
        "npm-run-all": "^4.1.5",
        "sass": "^1.58.3",
        "typescript": "~4.7.4",
        "vite": "^4.0.0",
        "vitest": "^0.25.6",
        "vue-tsc": "^1.0.12"
    }
}
