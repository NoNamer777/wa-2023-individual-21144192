{
    "tasksRunnerOptions": {
        "default": {
            "runner": "nx/tasks-runners/default",
            "options": {
                "cacheableOperations": ["build", "test", "lint"]
            }
        }
    },
    "targetDefaults": {
        "build": {
            "outputs": ["{workspaceRoot}/dist/"]
        },
        "test": {
            "outputs": ["{workspaceRoot}/coverage/"]
        }
    },
    "defaultBase": "main"
}
